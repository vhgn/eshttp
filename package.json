{
  "name": "eshttp",
  "private": true,
  "packageManager": "bun@1.3.2",
  "type": "module",
  "workspaces": [
    "apps/*",
    "libs/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "typecheck": "turbo run typecheck",
    "lint": "turbo run lint",
    "format": "turbo run format",
    "test": "turbo run test",
    "dev:cli": "bun run --filter 'eshttp' dev",
    "dev:desktop": "bun run --filter '@eshttp/desktop' dev"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.2",
    "@types/bun": "^1.3.9",
    "turbo": "^2.8.10",
    "typescript": "^5.8.2"
  }
}
