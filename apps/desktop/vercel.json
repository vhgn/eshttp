{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "vite",
  "buildCommand": "bun run build",
  "outputDirectory": "dist",
  "functions": {
    "api-build/**/*.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/auth/github/start",
      "dest": "/api-build/auth/github/start.js"
    },
    {
      "src": "/api/auth/github/callback",
      "dest": "/api-build/auth/github/callback.js"
    },
    {
      "src": "/api/auth/github/session",
      "dest": "/api-build/auth/github/session.js"
    },
    {
      "src": "/api/github/workspaces",
      "dest": "/api-build/github/workspaces.js"
    },
    {
      "src": "/api/github/commit",
      "dest": "/api-build/github/commit.js"
    },
    {
      "src": "/api/github/webhook",
      "dest": "/api-build/github/webhook.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
