{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.api.tsbuildinfo",
    "types": ["node"],
    "module": "CommonJS",
    "moduleResolution": "Node",
    "verbatimModuleSyntax": false,
    "isolatedModules": false,
    "declaration": false,
    "declarationMap": false,
    "sourceMap": false,
    "noEmit": false,
    "rootDir": "./api",
    "outDir": "./api-build"
  },
  "include": ["api/**/*.ts"]
}
