{
  "name": "@eshttp/cli",
  "private": false,
  "version": "0.1.0",
  "description": "CLI HTTP developer tool for eshttp workspaces",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "eshttp": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "biome check src test",
    "format": "biome format --write src test",
    "test": "bun test",
    "dev": "bun run src/index.ts"
  },
  "dependencies": {
    "eshttp-core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "typescript": "^5.8.2"
  }
}
